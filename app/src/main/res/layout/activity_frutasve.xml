<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".yatulve.Frutas"
    android:background="@drawable/fondofrutashve">

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="221dp"
        android:layout_height="89dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/titlefrutas" />

    <ImageView
        android:id="@+id/imgPregunta"
        android:layout_width="90dp"
        android:layout_height="62dp"

        android:layout_marginTop="12dp"
        app:layout_constraintStart_toEndOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:srcCompat="@drawable/interrogacionve"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imgUchuvas"
        android:layout_width="147dp"
        android:layout_height="264dp"
        android:layout_marginTop="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/uchuvave"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/titlefresas"
        android:layout_width="120dp"
        android:layout_height="69dp"
        android:layout_marginBottom="4dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/imgFresas"
        app:layout_constraintEnd_toEndOf="@+id/imgFresas"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgFresas"
        app:srcCompat="@drawable/titlefresas" />

    <ImageView
        android:id="@+id/imgFresas"
        android:layout_width="114dp"
        android:layout_height="215dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="172dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.152"
        app:layout_constraintStart_toEndOf="@+id/imgUchuvas"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/fresasve" />

    <ImageView
        android:id="@+id/imgMortino"
        android:layout_width="235dp"
        android:layout_height="168dp"
        android:layout_marginStart="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgFresas"
        app:layout_constraintTop_toBottomOf="@+id/titlemortino"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/arandanove" />

    <ImageView
        android:id="@+id/titleuchuvas"
        android:layout_width="150dp"
        android:layout_height="62dp"
        android:layout_marginEnd="36dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/imgUchuvas"
        app:layout_constraintEnd_toStartOf="@+id/imgFresas"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/titleuchuvas" />

    <ImageView
        android:id="@+id/titlemora"
        android:layout_width="140dp"
        android:layout_height="52dp"
        android:layout_marginBottom="12dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/imgMora"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.631"
        app:layout_constraintStart_toEndOf="@+id/imageView22"
        app:srcCompat="@drawable/titlemora" />

    <ImageView
        android:id="@+id/titlemortino"
        android:layout_width="127dp"
        android:layout_height="52dp"
        android:layout_marginTop="16dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toStartOf="@+id/imgMora"
        app:layout_constraintHorizontal_bias="0.414"
        app:layout_constraintStart_toEndOf="@+id/imgFresas"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:srcCompat="@drawable/titlemortino" />

    <ImageView
        android:id="@+id/imgMora"
        android:layout_width="143dp"
        android:layout_height="263dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toEndOf="@+id/imgMortino"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/morave" />

</androidx.constraintlayout.widget.ConstraintLayout>